<div class="class container">
<h1>Add a list</h1>
</div>

<div class="class container">
  <%= simple_form_for @list do |f| %>
    <%= f.input :name %>
    <%= f.button :submit, "Create a list", class: 'btn btn-success' %>  
  <% end %>
</div>

<div class='text-center'>
<%= link_to 'Back to my lists', lists_path, class: 'btn btn-outline-info' %>
</div>

